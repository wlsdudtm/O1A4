<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
					"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lg.team3.partyMember">
	<resultMap type="com.lg.team3.model.PartyMemberModel" id="partyMemberModel">
		<result column="_id" property="id" />
		<result column="member_id" property="memberId" />
		<result column="party_id" property="partyId" />
	</resultMap>

	<sql id="PartyMember_Column_List">
		member_id, party_id
	</sql>

	<insert id="addPartyMember" parameterType="com.lg.team3.model.PartyMemberModel">
		insert into party_member(
		<include refid="PartyMember_Column_List" />
		)
		VALUES(
		#{memberId}, #{partyId}
		)
	</insert>
</mapper>